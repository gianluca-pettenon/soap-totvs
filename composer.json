{
    "name": "gianluca-pettenon/soap-totvs",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Gianluca Pettenon",
            "email": "contact@gianlucapettenon.dev",
            "homepage": "https://gianlucapettenon.dev",
            "role": "Developer"
        }
    ],
    "scripts": {
        "start": "php -d display_errors=1 -d display_startup_errors=1 -d error_reporting=E_ALL -S 0.0.0.0:8080 -t .",
        "lint": "php vendor/bin/phpstan analyse",
        "test": "php vendor/bin/phpunit"
    },
    "require": {
        "php": ">=8.4",
        "laminas/laminas-soap": "^2.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "phpstan/phpstan": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "SoapTotvs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SoapTotvs\\Tests\\": "tests/"
        }
    }
}
